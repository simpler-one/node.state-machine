{
    "extends": ["tslint:recommended"],
    "rules": {
        "array-type": [true, "array"],
        "interface-name": [false],
        "no-consecutive-blank-lines": false,
        "whitespace": [true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type",
            "check-type-operator",
            "check-preblock"
        ]
    }
}